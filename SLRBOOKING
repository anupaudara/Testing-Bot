    $('label.btn').click(function () {
        var nic = 'Your NIC';
        var passport = 'Your Passport';
        if ($(this).text().trim() === 'NIC') {
            $("#nicL_reg").text(nic);
            $("#nic_reg").attr("placeholder", nic);
        } else {
            $("#nicL_reg").text(passport);
            $("#nic_reg").attr("placeholder", passport);
        }
    });

    function changeNICPattern(val) {
        if (val === 'NIC') {
            // $('#nicpp_').attr('pattern','(?:19|20)?\\d{2}(?:[0-35-8]\\d\\d(?<!(?:000|500|36[7-9]|3[7-9]\\d|86[7-9]|8[7-9]\\d)))\\d{4}(?:[vVxX])');
            $('#nic_reg').attr('pattern', '([0-9]{9}[x|X|v|V]|[0-9]{12})');
        } else {
            $('#nic_reg').attr('pattern', '[0-9NnPpLl]{5,}');
        }
    }

