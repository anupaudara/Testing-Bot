    function setIssuedDate(date) {
        console.log(date)
        var issuedDate = new Date(date);
        var maxTravelDate = new Date(issuedDate.getFullYear(), issuedDate.getMonth(), issuedDate.getDate() + 45);

        $('#travelDate_warrant').datepicker('destroy');

        datepickertravel = $('#travelDate_warrant').datepicker({
            minDate: issuedDate,
            format: 'yyyy-mm-dd',
            uiLibrary: 'bootstrap4',
            iconsLibrary: 'fontawesome',
            maxDate: maxTravelDate,
        });
    }

    function setTravelDate(date) {
        var travelDate = new Date(date);
        var today = new Date(new Date().getFullYear(), new Date().getMonth(), new Date().getDate());
        var minWarrantDate = new Date(travelDate.getFullYear(), travelDate.getMonth(), travelDate.getDate() - 14);
        var maxWarrantDate = new Date(travelDate.getFullYear(), travelDate.getMonth(), travelDate.getDate() + 14);

        if (minWarrantDate < today) {
            minWarrantDate = new Date(today.getFullYear(), today.getMonth(), today.getDate() + 4);
        }

        $('#datewarrant').datepicker('destroy');

        datepickerwarrant = $('#datewarrant').datepicker({
            minDate: minWarrantDate,
            format: 'yyyy-mm-dd',
            uiLibrary: 'bootstrap4',
            iconsLibrary: 'fontawesome',
            maxDate: maxWarrantDate,
        });
    }

    function setPensionTravelDate(date) {
        var travelDate = new Date(date);
        var today = new Date(new Date().getFullYear(), new Date().getMonth(), new Date().getDate());
        var minPensionDate = new Date(travelDate.getFullYear(), travelDate.getMonth(), travelDate.getDate() - 14);
        var maxPensionDate = new Date(travelDate.getFullYear(), travelDate.getMonth(), travelDate.getDate() + 14);

        if (minPensionDate < today) {
            minPensionDate = new Date(today.getFullYear(), today.getMonth(), today.getDate());
        }

        datepickerpensionwarrant = $('#pensionWarrantDate').datepicker({
            minDate: minPensionDate,
            format: 'yyyy-mm-dd',
            uiLibrary: 'bootstrap4',
            iconsLibrary: 'fontawesome',
            maxDate: maxPensionDate,
        });
    }

    function lightbox(idx) {
        //show the slider's wrapper: this is required when the transitionType has been set to "slide" in the ninja-slider.js
        var ninjaSldr = document.getElementById("ninja-slider");
        ninjaSldr.parentNode.style.display = "block";

        nslider.init(idx);

        var fsBtn = document.getElementById("fsBtn");
        fsBtn.click();
    }

    function fsIconClick(isFullscreen, ninjaSldr) { //fsIconClick is the default event handler of the fullscreen button
        if (isFullscreen) {
            ninjaSldr.parentNode.style.display = "none";
        }
    }

    function matchCustom(params, data) {
        // If there are no search terms, return all of the data
        if ($.trim(params.term) === '') {
            return data;
        }

        // Do not display the item if there is no 'text' property
        if (typeof data.text === 'undefined') {
            return null;
        }

        // `params.term` should be the term that is used for searching
        // `data.text` is the text that is displayed for the data object
        if (data.text.toUpperCase().indexOf(params.term.toUpperCase()) > -1) {
            var modifiedData = $.extend({}, data, true);
            modifiedData.text += ' (matched)';

            // You can return modified objects from here
            // This includes matching the `children` how you want in nested data sets
            return modifiedData;
        }

        // Return `null` if the term should not be displayed
        return null;
    }

