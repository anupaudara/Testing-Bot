<script>

    var input = document.querySelector("#phone_flag");
    var intl = intlTelInput(input, {
        // allowDropdown: false,
        // autoHideDialCode: false,
        // autoPlaceholder: "off",
        // dropdownContainer: document.body,
        // excludeCountries: ["us"],
        formatOnDisplay: true,
        // geoIpLookup: function(callback) {
        //   $.get("http://ipinfo.io", function() {}, "jsonp").always(function(resp) {
        //     var countryCode = (resp && resp.country) ? resp.country : "";
        //     callback(countryCode);
        //   });
        // },
        // hiddenInput: "full_number",
        initialCountry: "lk",
        // localizedCountries: { 'de': 'Deutschland' },
        // nationalMode: false,
        // onlyCountries: ['us', 'gb', 'ch', 'ca', 'do'],
        // placeholderNumberType: "MOBILE",
        // preferredCountries: ['cn', 'jp'],
        customPlaceholder: function (selectedCountryPlaceholder, selectedCountryData) {
            return "e.g. " + selectedCountryPlaceholder;
        },
        separateDialCode: true,
        utilsScript: "mobileflag/js/utils.js",
    });

    var reset = function () {
        input.classList.remove("error");
        // $('.invalid-feedback-cus').hide();
    };

    //Important Notice modal function
    function openWarrantModal() {
        $('#warrantModal').modal('show');
    }
    function openPensionWarrantModal() {
        $('#pensionWarrantModal').modal('show');
    }

    function validateMobileNo() {
        // reset();
        if (input.value.trim()) {
            if (intl.isValidNumber()) {
                // $('.invalid-feedback-cus').hide();
                $('#mobile_contact').val(intl.getNumber());
                $(input).attr('pattern', '.{,}');
            } else {
                input.classList.add("error");
                $(input).attr('pattern', '.{99,}');
                $('#mobile_contact').val("");
                // $('.invalid-feedback-cus').show();
            }
        } else {
            input.classList.add("error");
            $(input).attr('pattern', '.{99,}');
            // $('.invalid-feedback-cus').show();
            $('#mobile_contact').val("");
        }
    }

    input.addEventListener("countrychange", function () {
        input.classList.add("error");
        $(input).attr('pattern', '.{99,}');
        $('#mobile_contact').val("");
        $(input).val("");
        // $('.invalid-feedback-cus').show();
    });

    var today, warrantday, maxwarrantday, datepicker, datepickerret, datepickerwarrant, datepickerpensionwarrant;

    // localStorsge.setItem('login_init', true);
    // localStorsge.setItem('register_init', false);

    function login_prof(val) {
        if (val == 'login') {
            var form = document.getElementById('login_Modal');
            if (form.checkValidity()) {
                $('#login_').show();
                $('#profile_').hide();
            }
            // localStorsge.setItem('login_init', true);
            // localStorsge.setItem('register_init', false);

        } else {
            var form = document.getElementById('register_Modal');
            if (form.checkValidity()) {
                $('#login_').hide();
                $('#profile_').show();
            }

            // localStorsge.setItem('login_init', false);
            // localStorsge.setItem('register_init', true);
        }
    }

    $(document).ready(function () {
        /*
            48-57 - (0-9)Numbers
            65-90 - (A-Z)
            97-122 - (a-z)
            8 - (backspace)
            32 - (space)
            */

        // handling dynamic contact info
        var contactAdditionInfo = document.getElementById('additionalContent');
        contactAdditionInfo.style.display = 'none';

        $("select#subject_contact").change(function () {
            if ($(this).children("option:selected").val() == 'PI') {
                document.getElementById("exampleInputNic").required = true;
                document.getElementById("exampleInputCardNo").required = true;
                document.getElementById("exampleInputAmount").required = true;
                document.getElementById("exampleInputDate").required = true;
                contactAdditionInfo.style.display = 'block';
            } else {
                $('#exampleInputNic').removeAttr('required');
                $('#exampleInputCardNo').removeAttr('required');
                $('#exampleInputAmount').removeAttr('required');
                $('#exampleInputDate').removeAttr('required');
                contactAdditionInfo.style.display = 'none';
            }
        });

        $("#dateOne").keypress(function (e) {
            var keyCode = e.which;
            // Not allow special
            if (!(keyCode >= 44 && keyCode <= 57)) {
                e.preventDefault();
            }
        });
        $('#dateOne').on("cut copy paste", function (e) {
            e.preventDefault();
        });
        $("#dateRet").keypress(function (e) {
            var keyCode = e.which;
            // Not allow special
            if (!(keyCode >= 44 && keyCode <= 57)) {
                e.preventDefault();
            }
        });
        $('#dateRet').on("cut copy paste", function (e) {
            e.preventDefault();
        });
        $("#datewarrant").keydown(function (e) {
            e.preventDefault();
        });
        $('#datewarrant').on("cut copy paste", function (e) {
            e.preventDefault();
        });
        $("#issuedDate_warrant").keydown(function (e) {
            e.preventDefault();
        });
        $('#issuedDate_warrant').on("cut copy paste", function (e) {
            e.preventDefault();
        });
        $("#travelDate_warrant").keydown(function (e) {
            e.preventDefault();
        });
        $('#travelDate_warrant').on("cut copy paste", function (e) {
            e.preventDefault();
        });

        $("#exampleInputEmail2").keypress(function (e) {
            var keyCode = e.which;
            // Not allow special
            if (!((keyCode >= 39 && keyCode <= 57) || (keyCode >= 64 && keyCode <= 90) || (keyCode >= 97 && keyCode <= 122)) && keyCode != 8 && keyCode != 32 && keyCode != 95) {
                e.preventDefault();
            }
        });
        $('#exampleInputEmail2').on("cut copy paste", function (e) {
            e.preventDefault();
        });
        $("#exampleInputEmail1").keypress(function (e) {
            var keyCode = e.which;
            // Not allow special
            if (!((keyCode >= 39 && keyCode <= 57) || (keyCode >= 64 && keyCode <= 90) || (keyCode >= 97 && keyCode <= 122)) && keyCode != 8 && keyCode != 32 && keyCode != 95) {
