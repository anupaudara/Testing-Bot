    function openDefaultTab() {
        document.querySelector('.search-tab button.tablinks:first-child').click();
    }

    function openUserType(evt, userType) {
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("search-tab-content");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablinks");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].classList.remove("active");
        }

        document.getElementById(userType).style.display = "block";
        evt.currentTarget.classList.add("active");

        if (userType === 'warrant-user' || userType === 'p-warrant-user') {
            document.getElementById('searchInfo').style.display = "block";
        } else {
            document.getElementById('searchInfo').style.display = "none";
        }
        //todo
        if (userType === 'warrant-user' || userType === 'p-warrant-user') {

            document.getElementById('subtext').style.display = "none";

        } else {
            document.getElementById('subtext').style.display = "block";
        }
        //todo
    }

    // OTP popUp modal
    function showOtpModal() {
        event.preventDefault();

        var mobileNumberInput = document.getElementById('mobilenumber_warrant');
        var mobileNumberError = document.getElementById('mobileNumberError');
        if (mobileNumberInput.checkValidity()) {
            $.ajax({
                url: 'api/OTP/send',
                method: 'POST',
                dataType: 'json',
                data: {
                    mobileNumber: $('#mobilenumber_warrant').val()
                },
                success: function (data) {
                    $('#reqID').val(data.ref_id);
                    $('#otpModal').modal('show');
                    document.getElementById("displayMobileNumber").textContent = mobileNumberInput.value;
                    mobileNumberError.style.display = 'none';
                },
                error: function (xhr, status, error) {
                    console.error('Error fetching data. Status:', status, 'Error:', error);
                }
            });

        } else {
            mobileNumberError.style.display = 'block';
        }
    }
    function showPensionOtpModal() {
        event.preventDefault();
        var pensionWarrantNumberInput = document.getElementById('input_pension_warrantNumber');
        var nicInput = document.getElementById('input_pension_nic');

        var pensionWarrantError = $('#pensionWarrantError');

        pensionWarrantError.hide();

        var pensionWarrantValidationError = $('#pensionWarrantValidationError');
        pensionWarrantValidationError.hide();

        if (pensionWarrantNumberInput.checkValidity() && nicInput.checkValidity()) {
            $('.pension-verify-btn').prop('disabled', true);
            // AJAX form submission
            $.ajax({
                url: '/mtktwebslr/validate-pensioner',
                method: 'POST',
                contentType: 'application/json',
                data: JSON.stringify({
                    tripNumber: pensionWarrantNumberInput.value,
                    nicNumber: nicInput.value
                }),
                success: function (data) {
                    console.log(data);
                    $('#pensionReqID').val(data.referenceId);
                    $('#pension_warrantNumber').val(data.tripNo);
                    $('#pension_nic').val(data.nic);
                    pensionWarrantValidationError.hide();
                },
                error: function (xhr, status, error) {
                    console.error('Error fetching data. Status:' + status + ' Error:' + error);
                    var errorMessage = xhr.responseJSON ? xhr.responseJSON.msg : 'An unexpected error occurred. Please try again.';

                    pensionWarrantError.text(errorMessage);
                    pensionWarrantError.show();
                },
                complete: function () {
                    if ($('#pensionReqID').val() && $('#pension_warrantNumber').val() && $('#pension_nic').val()) {
                        $('#pensionOtpModal').modal('show');
                    }
                    $('.pension-verify-btn').prop('disabled', false);

                }
            });
        }else {
            pensionWarrantValidationError.show();
        }
    }

    const otpInputs = document.querySelectorAll('.otp-digit');
    const pensionOtpInputs = document.querySelectorAll('.pension-otp-digit');

    otpInputs.forEach((input, index) => {
        input.addEventListener('input', (event) => {
            const value = event.target.value;
            if (value && index < otpInputs.length - 1) {
                otpInputs[index + 1].focus();
            }
        });

        input.addEventListener('keydown', (event) => {
            if (event.key === 'Backspace' && index > 0 && !input.value) {
                otpInputs[index - 1].focus();
            }
        });
    });
    pensionOtpInputs.forEach((input, index) => {
        input.addEventListener('input', (event) => {
            const value = event.target.value;
            if (value && index < pensionOtpInputs.length - 1) {
                pensionOtpInputs[index + 1].focus();
            }
        });

        input.addEventListener('keydown', (event) => {
            if (event.key === 'Backspace' && index > 0 && !input.value) {
                pensionOtpInputs[index - 1].focus();
            }
        });
    });

    function resendPensionOTP() {
        pensionOtpInputs.forEach((input) => {
            input.value = '';
        });
        pensionOtpInputs[0].focus();

        var pensionWarrantNumberInput = document.getElementById('input_pension_warrantNumber');
        var nicInput = document.getElementById('input_pension_nic');
        var pensionReferenceNo = document.getElementById('pensionReqID');

        $.ajax({
            url: '/mtktwebslr/resend-otp',
            method: 'POST',
            contentType: 'application/json',
            data: JSON.stringify({
                tripNumber: pensionWarrantNumberInput.value,
                nicNumber: nicInput.value,
                referenceId: pensionReferenceNo.value
            }),
            success: function (data) {

            },
            error: function (xhr, status, error) {
                console.error('Error fetching data. Status:', status, 'Error:', error);
            }
        });
    }

    function verifyOTP() {
        $('#verifyBtnText').addClass('d-none');
        $('#verifyBtnLoader').removeClass('d-none');
        $('.verify-proceed-btn').prop('disabled', true);

        const otpValue = Array.from(otpInputs).map(input => input.value).join('');
        // console.log('Entered OTP:', otpValue);
        var reqID = $('#reqID').val();
        // console.log('Request ID:', reqID);
        var mobileNumberInput = $('#mobilenumber_warrant').val();
        // console.log('mobileNumberInput:', mobileNumberInput)
        $.ajax({
            url: 'api/OTP/verify',
            method: 'POST',
            dataType: 'json',
            data: {
                mobileNumber: mobileNumberInput,
                otp: otpValue,
                reqID: reqID
            },
            success: function (data) {
                $('#verifyBtnText').removeClass('d-none');
                $('#verifyBtnLoader').addClass('d-none');
                $('.verify-proceed-btn').prop('disabled', false);
                // console.log("OTP status", data.status);
                if (data.status == 1000) {
                    $('#otpModal').modal('hide');
                    console.log("modal closed");
                    $('#mobilenumber_warrant').prop('readonly', true);
                    $('.verify-btn').prop('disabled', true);

                    $('#mobileVerifiedIcon').show();
                    $('#mobileNumberNotVerified').hide();
                    $('.warrant-search-btn').prop('disabled', false);

                } else if (data.status == 1007) {
                    $('#mobileVerifiedIcon').hide();
                    $('#mobileVerifiedError').show();
                } else {
                    console.error(data.message);
                    $('#mobileVerifiedIcon').hide();
                }

            },
            error: function (xhr, status, error) {
                console.error('Error fetching data. Status:', status, 'Error:', error);
            }
        });
    }
    function verifyPensionOTP() {
        $('#verifyBtnText').addClass('d-none');
        $('#verifyBtnLoader').removeClass('d-none');
        $('.verify-proceed-btn').prop('disabled', true);

        const otpValue = Array.from(pensionOtpInputs).map(input => input.value).join('');

        var refID = $('#pensionReqID').val();
        var tripNo = $('#pension_warrantNumber').val();
        var nic = $('#pension_nic').val();
        $.ajax({
            url: '/mtktwebslr/verify-details',
            method: 'POST',
            contentType: 'application/json',
            data: JSON.stringify({
                referenceId: refID,
                nic: nic,
                tripNo: tripNo,
                otp: otpValue
            }),
            success: function (data) {
                $('#verifyBtnText').removeClass('d-none');
                $('#verifyBtnLoader').addClass('d-none');
                $('.verify-proceed-btn').prop('disabled', false);
                console.log("OTP status", data.status);
                if (data.status == 200) {
                    var pensionDataObj = {
                        referenceId: refID,
                        otp: otpValue,
                        tripDetails: data.data.tripDetails,
                        passengerDetails: data.data.passengerDetails
                    }
                    $('#personData').val(JSON.stringify(pensionDataObj));

                    $('#pension_warrantNumber').val(data.data.tripDetails.tripId);
                    $('#pension_classType').val(data.data.tripDetails.classType);

                    // var issueDateStr = data.data.tripDetails.issuedDate;
                    var issueDateParts = data.data.tripDetails.issuedDate.split('-');
                    var issueDate = new Date(issueDateParts[0], issueDateParts[1] - 1, issueDateParts[2]);
                    $('#pension_warrantIssuingDate').val(issueDate.toLocaleDateString());

                    // var proposedTravelDateStr = data.data.tripDetails.proposedTravelDate;
                    var proposedTravelDateParts = data.data.tripDetails.proposedTravelDate.split('-');
                    var proposedTravelDate = new Date(proposedTravelDateParts[0], proposedTravelDateParts[1] - 1, proposedTravelDateParts[2]);
                    $('#pension_proposedTravelDate').val(proposedTravelDate.toLocaleDateString());
                    setPensionTravelDate(proposedTravelDate);

                    $('#pension_nic').val(data.data.tripDetails.pensionerDetails.nic);
                    $('#pension_title').val(data.data.tripDetails.pensionerDetails.title);

                    var fullName = data.data.tripDetails.pensionerDetails.name;
                    var firstSpaceIndex = fullName.indexOf(" ");
                    var firstPart = "";
                    var secondPart = " ";

                    if (firstSpaceIndex !== -1) {
                        firstPart = fullName.substring(0, firstSpaceIndex);
                        secondPart = fullName.substring(firstSpaceIndex + 1);
                    } else {
                        firstPart = fullName;
                    }

                    $('#pension_firstName').val(firstPart);
                    $('#pension_lastName').val(secondPart);

                    $('#pension_firstName').val(firstPart);
                    if (data.data.tripDetails.pensionerDetails.gender === 'Male' ||
                        data.data.tripDetails.pensionerDetails.gender === 'male' ||
                        data.data.tripDetails.pensionerDetails.gender === 'MALE') {
                            $('#pension_gender').val('Male');
                    }else if (data.data.tripDetails.pensionerDetails.gender === 'Female' ||
                        data.data.tripDetails.pensionerDetails.gender === 'female' ||
                        data.data.tripDetails.pensionerDetails.gender === 'FEMALE') {
                            $('#pension_gender').val('Female');
                    }
                    $('#pension_mobileNumber').val(data.data.tripDetails.pensionerDetails.phoneNumber);
                    $('#fromstation_p_warrant').val(data.data.tripDetails.fromStationName);
                    $('#fromStationId_p_warrant').val(data.data.tripDetails.fromStationId);
                    $('#tostation_p_warrant').val(data.data.tripDetails.toStationName);
                    $('#toStationId_p_warrant').val(data.data.tripDetails.toStationId);

                    var passengerList = data.data.passengerDetails;
                    $('#pension_passengerList').val(JSON.stringify(passengerList));
                    populatePassengerTable();
                    console.log("passengerList - " + passengerList);

                    $('#pensionOtpModal').modal('hide');
                    $('#pension_validation_form').hide();
                    $('#isPensionWarrantValidated').val(true);
                    $('#pension_input_form').show();

                }
                else {
                    console.error(data.message);
                    $('#pMobileVerifiedError').show();
                    $('#isPensionWarrantValidated').val(false);
                    $('.verify-proceed-btn').prop('disabled', false);
                }
            },
            error: function (xhr, status, error) {
                console.error('Error fetching data. Status:', status, 'Error:', error);
                $('#pMobileVerifiedError').show();
                $('#isPensionWarrantValidated').val(false);
                $('.verify-proceed-btn').prop('disabled', false);
            }
        });
    }

    function populatePassengerTable() {
        var passengerListStr = $('#pension_passengerList').val();
        console.log("Hidden input value: " + passengerListStr);

        if (passengerListStr) {
            try {
                var passengerList = JSON.parse(passengerListStr);
                var tableBody = $('#passengerTable tbody');
                tableBody.empty();

                passengerList.forEach(function(person) {
                    var uniqueId = person.nic;
                    if(person.idType == 11){
                        uniqueId = person.personId;
                    } else {
                        uniqueId = person.nic;
                    }

                    var row = '<tr>' +
                        '<td>' + person.personId + '</td>' +
                        '<td>' + person.name + '</td>' +
                        '<td>' + uniqueId + '</td>' +
                        '<td>' + person.gender + '</td>' +
                        '<td>' + person.relation + '</td>' +
                        '<td><input type="checkbox" name="selectedPassengers" value="' + person.personId + '"></td>' +
                        '</tr>';
                    tableBody.append(row);
                });

                $(document).on('change', '#passengerTable input[name="selectedPassengers"]', function() {
                    updateSelectedPassengers();
                });

            } catch (error) {
                console.error("Error parsing JSON: ", error);
            }
        } else {
            console.error("No data found in the hidden input field.");
        }
    }
    function updateSelectedPassengers() {
        var selectedPassengers = [];
        var maxPassengers = 5;
        var minPassengers = 1;

        $('#passengerTable tbody input[name="selectedPassengers"]:checked').each(function() {
            var personId = $(this).val();
            var row = $(this).closest('tr');

            var person = {
                personId: personId,
                name: row.find('td:eq(1)').text(),
                nic: row.find('td:eq(2)').text(),
                gender: row.find('td:eq(3)').text(),
                relation: row.find('td:eq(4)').text(),
                isSelected: 1
            };
            selectedPassengers.push(person);
        });

        // Perform validation
        var selectedCount = selectedPassengers.length;

        if (selectedCount > maxPassengers) {
            $('#pension_moreThan5Passengers').show();
        } else if (selectedCount < minPassengers) {
            $('#pension_emptyPassengers').show();
        } else {
            $('#pension_emptyPassengers').hide();
            $('#pension_moreThan5Passengers').hide();
        }

        var passengerList = JSON.parse($('#pension_passengerList').val());
        var selectedPassengerUpdateList = [];

        passengerList.forEach(function(person) {
            selectedPassengers.forEach(function(selectedPerson) {
                if (selectedPerson.personId == person.personId) {
                    person.isSelected = 1;
                    selectedPassengerUpdateList.push(person);
                }
            });
        });

        var warrentDetails = JSON.parse($('#personData').val());
        warrentDetails.passengerDetails = selectedPassengerUpdateList;

        $('#personData').val(JSON.stringify(warrentDetails));

        // Update the hidden field with the selected passenger count
        $('#selectedPassengerCount').val(selectedCount);

        console.log("Selected passengers: " + JSON.stringify(selectedPassengerUpdateList));
    }


    function otpModalCloseButton() {
        $('#otpModal').modal('hide');
    }
    function pensionOtpModalCloseButton() {
        $('#pensionOtpModal').modal('hide');
    }

    $('.search-form').submit(function(event) {
        if ( $('#g-recaptcha-response').val() === '' ) {
            event.preventDefault();
            // alert('Please check the recaptcha');
            $('#recaptcha-error').show();
        }else {
            $('#recaptcha-error').hide();
        }
    });
    function resetSearch() {
        $('#fromstation').val('');
        $('#tostation').val('');
        $('#fromstation').select2().trigger('change');
        $('#tostation').select2().trigger('change');
        $('#dateOne').val('');
        $('#passengers').val('');
        $('#dateRet').val('');
        $('#dateRet').prop('disabled', true);
        $('#gridCheck').prop('checked', false);
        $('#pickReturn').hide();
        $('#dateRet').prop('required', false);
        grecaptcha.reset();
    }

    function handleFormSubmission(event) {
        var mobileVerifiedIcon = $('#mobileVerifiedIcon');
        var submitButton = $('.warrant-search-btn');

        if (mobileVerifiedIcon.is(':visible')) {
            submitButton.prop('disabled', false);
        } else {
            event.preventDefault();
            $('#mobileNumberNotVerified').show();
            submitButton.prop('disabled', true);
        }
    }
    $('.warrant-search-form').submit(handleFormSubmission);
    function resetWarrantSearch() {
        $('#fromstation_warrant').val('');
        $('#tostation_warrant').val('');
        $('#fromstation_warrant').select2().trigger('change');
        $('#tostation_warrant').select2().trigger('change');
        $('#datewarrant').val('');
        $('#passengers_warrant').val('');
        $('#warrantNumber_warrant').val('');
        $('#departmentcode_warrant').val('');
        $('#classType_warrant').val('');
        $('#classType_warrant').select2().trigger('change');
        $('#issuedDate_warrant').val('');
        $('#travelDate_warrant').val('');
        $('#nic_warrant').val('');
        // $('#title_warrant').val('');
        // $('#title_warrant').select2().trigger('change');
        $('#fname_warrant').val('');
        $('#lname_warrant').val('');
        $('#gendertype_warrant').val('');
        $('#gendertype_warrant').select2().trigger('change');
        $('#mobilenumber_warrant').val('');
        $('#mobilenumber_warrant').prop('readonly', false);
        $('.verify-btn').prop('disabled', false);
        //
        $('#mobileVerifiedIcon').hide();
        // $('#mobileNumberNotVerified').show();
        $('.warrant-search-btn').prop('disabled', true);

        $('.otp-digit').val('');
    }

    function handlePensionFormSubmission(event) {
        var pension_emptyPassengers_error = $('#pension_emptyPassengers');
        var passengerCount = parseInt($('#selectedPassengerCount').val(), 10);

        console.log("Passenger Count - " + passengerCount);

        if (isNaN(passengerCount) || passengerCount <= 0 || passengerCount > 5) {
            event.preventDefault();
        }
    }
    $('.pension-warrant-search-form').submit(handlePensionFormSubmission);
    function resetPensionWarrantSearch() {
        $('#pensionWarrantDate').val('');
        $('#selectedPassengerCount').val('');
        $('#passengerTable tbody input[name="selectedPassengers"]').prop('checked', false);
        var passengerList = JSON.parse($('#pension_passengerList').val() || '[]');
        passengerList.forEach(function(person) {
            person.isSelected = 0;
        });
        $('#selectedPassengerCount').val(0);
        $('#pension_emptyPassengers').hide();

        var warrentDetails = JSON.parse($('#personData').val() || '{}');
        warrentDetails.passengerDetails = passengerList;
        $('#personData').val(JSON.stringify(warrentDetails));
    }


    const warrantNumberInput = document.getElementById('warrantNumber_warrant');
    warrantNumberInput.addEventListener('input', function () {
        this.value = this.value.toUpperCase();
        // console.log("Uppercase values - ", this.value);
    });

    document.addEventListener('DOMContentLoaded', function() {
        var inputFields = document.querySelectorAll('input[type=text]');
        inputFields.forEach(function(input) {
            var previousValue = input.value; // Store the initial value
            input.addEventListener('change', function() {
                if (input.value === previousValue) {
                    // If the value remains the same, it's likely autofill
                    input.value = ''; // Clear the field
                } else {
                    previousValue = input.value;
                }
            });
        });
    });
</script>

</html>
