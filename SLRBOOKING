    // new login and register methods
    $(document).ready(function () {

        // language
        var default_lang = 'en_US';
        if (default_lang === 'en_US') {
            $('#current_lang').text("English");
        } else if (default_lang === 'sin') {
            $('#current_lang').text("සිංහල");
        } else if (default_lang === 'tam') {
            $('#current_lang').text("தமிழ்");
        }

        // check login
        var isLogged = ('' == 'true');

        if (isLogged) {
            $('.after-login').show();
            $('.before-login').hide();
        } else {
            $('.after-login').hide();
            $('.before-login').show();
        }

        $('.toast-0').toast({
            'autohide': false,
            'delay': 4000
        });
        $('.toast-1').toast({
            'autohide': false,
            'delay': 4000
        });
        // toast language and profile
        var toastShow = false;
        $('.toast').on('show.bs.toast', function () {
            toastShow = true;
        });
        $('.toast').on('hidden.bs.toast', function () {
            toastShow = false;
        });

        $(window).click(function (e) {
            if (e.target.className.includes("navlanguage-tab")) {
                $('.toast-0').toast('show');
                $('.toast-1').toast('hide');
            } else if (e.target.className.includes("profile") || e.target.className.includes("fa-user-circle")) {
                $('.toast-1').toast('show');
                $('.toast-0').toast('hide');
            } else {
                if (toastShow) {
                    $('.toast-0').toast('hide');
                    $('.toast-1').toast('hide');
                }
            }
        });


        var isMessage = ''
        var method = ''

        if (isMessage) {
            if (method === 'LOGIN') {
                $('#loginMessageT').toast({
                    'autohide': true,
                    'delay': 30000
                });
                $('#loginMessageT').toast('show');
            } else if (method === 'REG') {
                $('#registerMessageT').toast({
                    'autohide': true,
                    'delay': 30000
                });
                $('#registerMessageT').toast('show');
            } else if (method === 'LOGOUT') {
                $('#logoutMessageT').toast({
                    'autohide': true,
                    'delay': 30000
                });
                $('#logoutMessageT').toast('show');
            } else if (method === 'PASSRESET') {
                $('#passresetMessageT').toast({
                    'autohide': true,
                    'delay': 30000
                });
                $('#passresetMessageT').toast('show');
            } else if (method === 'FPASSLINK') {
                $('#fpasslinkMessageT').toast({
                    'autohide': true,
                    'delay': 30000
                });
                $('#fpasslinkMessageT').toast('show');
            } else if (method === 'FPASSRESET') {
                $('#fpassresetMessageT').toast({
                    'autohide': true,
                    'delay': 30000
                });
                $('#fpassresetMessageT').toast('show');
            } else if (method === 'BLACKLIST') {
                $('#blacklistedWarrant').toast({
                    'autohide': true,
                });
                $('#inputMessageError').toast('show');
            } else if (method === 'INPUT') {
                $('#inputMessageT').toast({
                    'autohide': true,
                    'delay': 30000
                });
                $('#inputMessageT').toast('show');
            }else if (method === 'CONTACT') {
                $('#contactMessageT').toast({
                    'autohide': true,
                    'delay': 30000
                });
                // $('#contactMessageT').toast('show');
                $('#msgContactModal').modal('show');
            }else if (method === 'MCASH') {
                $('#mCashMessageT').toast({
                     'autohide': true,
                     'delay': 90000
                });
                $('#mCashMessageT').toast('show');
            }else if(method==='AddToBill'){

                $('#addToBillMsgT').toast({
                     'autohide': true,
                     'delay': 90000
                });
                $('#addToBillMsgT').toast('show');
            }
        }


    });

    var $dropdown1 = $("select[name='fromSt']");
